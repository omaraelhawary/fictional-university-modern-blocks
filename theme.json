{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 3,
    "customTemplates": [
        {
            "name": "emptycanvas",
            "postTypes": [
                "page"
            ],
            "title": "Empty Canvas"
        }
    ],
    "styles": {
        "blocks": {
            "core/button": {
                "color": {
                    "text": "#FFFFFF",
                    "background": "var(--wp--preset--color--primary)"
                }
            }
        }
    },
    "settings": {
        "color": {
            "palette": [
                {
                    "slug": "primary",
                    "color": "#0d3b66",
                    "name": "Primary"
                },
                {
                    "slug": "secondary",
                    "color": "#ee964b",
                    "name": "Secondary"
                },
                {
                    "slug": "background",
                    "color": "#FFFFFF",
                    "name": "background"
                },
                {
                    "slug": "foreground",
                    "color": "#333333",
                    "name": "Foreground"
                }
            ]
        },
        "layout": {
            "contentSize": "840px"
        }
    }
}