{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "ourblocktheme/genericbutton",
  "title": "Generic Button",
  "editorScript": "file:./index.js",
  "attributes": {
    "text": {
      "type": "string"
    },
    "size": {
      "type": "string",
      "default": "large"
    },
    "linkObj": {
      "type": "object",
      "default": {
        "url": "#"
      }
    },
    "colorName": {
      "type": "string",
      "default": "blue"
    }
  }
}